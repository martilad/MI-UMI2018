    "import numpy as np\n",
    "x = 10\n",
    "y = 20\n",
    "matrixDensity = 0.5\n",
    "n = int(x * y * matrixDensity)\n",
    "print(n)\n",
    "space = np.zeros((x*y), dtype=int)\n",
    "rngX = np.random.choice(range(20), 20, replace=False)\n",
    "rngX = np.random.randint(x*y, size=n)\n",
    "for i in rngX:\n",
    "    space[i] = 1\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "space.reshape(x, y)\n",
    "\n"
